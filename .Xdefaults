Xft.dpi: 96
Xft.hinting: true
Xft.antialias: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

Emacs.font: -*-terminus-medium-*-*-*-16-*-*-*-*-*-*-*

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! URxvt settings
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

URxvt*foreground: white
URxvt*path: /home/fally/.config/awesome/wallpapers
!URxvt*backgroundPixmap: seasons_cubbed.jpg;100x0+0+0:root
URxvt*background: #222222

URxvt*font: xft:Terminus:pixelsize=14:antialias=true

URxvt*geometry: 80x25
URxvt*perl-ext: tabbed
URxvt*tintColor: Gray
!URxvt*transparent: true
!URxvt*shading: 20
URxvt*fading: 10
URxvt*fadeColor: purple
URxvt*saveLines: 10000
URxvt*termName: xterm
URxvt*scrollBar: false
URxvt*visualBell: false
URxvt*urgentOnBell: false
URxvt*border: 1
! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! Настройка сочетаний Ctrl(+Shift) со стрелками.
URxvt*keysym.C-Left: \033[1;5D
URxvt*keysym.C-Right: \033[1;5C
URxvt*keysym.C-Down: \033[1;5B
URxvt*keysym.C-Up: \033[1;5A
URxvt*keysym.C-S-Left: \033[1;6D
URxvt*keysym.C-S-Right: \033[1;6C
URxvt*keysym.C-S-Up: \033[1;6B
URxvt*keysym.C-S-Down: \033[1;6A
URxvt*keysym.Home: \033[7~
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:  firefox
URxvt.matcher.button:   1
URxvt.colorUL:      #86a2be
!Colors
